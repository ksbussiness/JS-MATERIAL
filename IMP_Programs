#.11 Sum of an individual Number of a Numbers:-  // 6753==> 21
function individualSum(n)  {

let s=0
while (n!=0) {
   let d=n%10
   s=s+d
   n=parseInt(n/10)
}
console.log("The sum of numbers of an number",s);
}

individualSum(675)


