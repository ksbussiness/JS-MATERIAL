# 14  G.C.D of the given numbers :

function findGCD(a,b) {
let r
while(true) {
    r=a%b;
    if (r===0){
        break
    }
    a=b
    b=r
}
console.log("The gcd of the numbers ",b)
    
}
findGCD(15,20)

