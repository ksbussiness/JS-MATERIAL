# 18. on function call returns 1 more than the previous value every subsequent time it is called (n, n + 1, n + 2, etc).

var createCounter = function(n) {
    let count=0
    
    return function() {
        let res=n+count;
         count=count+1
         return res
       
    };
};
 
//  const counter = createCounter(10)
//   counter() // 10
//   counter() // 11
//   counter() // 12


