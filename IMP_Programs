# 15.  remove duplicate elements from an array:

function removeDuplicates(arr) {
    
    for ( let i=0;i<=arr.length;i++) {
        for( let j=i+1;j<arr.length;j++){
            if (arr[i]==arr[j]) {
                arr.splice(j,1)
            }
        }
    }
    console.log(arr)
}

removeDuplicates([1,3,4,2,1,3,2])

................................................................................................................................. >
# 16. To find the number times given element is repeated in an Array;

function repatedCount(arr,e) {
    let i=0
    let count=0
    while (i<arr.length) {
        if (arr[i]==e) {
            count=count+1
        }
        i++
    }
    console.log(count)
}
repatedCount([2,3,9,4,9,9,],9)

