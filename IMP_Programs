# 10  Fibonacci sequence:
 function fibonacciSer(n) {
     let a=-1;
     let b=1
     
    for (let i=1;i<=n;i++) {
        let c=a+b
        console.log(c)
        a=b
        b=c
        
    }
     
 }
 fibonacciSer(10)

