# 13. given number is palindrome or not find
function isNumPalindomre(n) {
    let t=n
    let rev=0
    while (n!=0){
        rev=rev*10+n%10;
        n=parseInt(n/10);
    }
    if ( rev===t) {
        console.log("Yes the number is palindrome")
    }
    else{
        console.log("No the number is not palindorme")
    }
}
isNumPalindomre(525)


