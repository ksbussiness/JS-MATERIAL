# 19.  array chunk Given an array arr and a chunk size size, return a chunked array.

ex:
Input: arr = [1,2,3,4,5], size = 1
Output: [[1],[2],[3],[4],[5]]


function arrayAchunk(arr,n) {
    // if (arr.length===0) {
    //     return []
    // }
    // else if (arr.length===1) {
    //     return [arr]
    // }
    // else {
    //         if(arr.length>n){
    //             let res=[],i=0
    //             let cc= parseInt( (arr.length)/n);
    //               while (i<cc) {
    //                 res.push(arr.slice(0,n))
    //                 arr.splice(0,n)
    //                 i++
    //         }
    //          if (n===1) {
    //             res.pop()
    //          }
    //          return res
    //         }
    //         else {
    //             return arr
    //         }
    //     }
    
    
    // let res = [];
    // for (let i = 0; i < arr.length; i += n) {
    //     res.push(arr.slice(i, i + n));
    // }
    // return res;
    
    let res=[];
    for ( let i=0;i<arr.length;i=i+n) {
            res.push(arr.slice(i,i+n))
    }
    
    return res;
}

console.log(arrayAchunk(["5","7","2","1","0","-5"],3))

