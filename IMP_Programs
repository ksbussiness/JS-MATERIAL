# 2. Max and Min of the array

function findMax(arr) {
    if( arr.length===0){
return { min: undefined, max: undefined };
    }
    let min=arr[0];
    let max=arr[0];
    
    for ( let i=0;i<=arr.length;i++){ 
        if (arr[i]<min) {
            min=arr[i]
        }
        else if (arr[i]>max) {
            max=arr[i]
        }
    }
    return { min, max};
}
console.log(findMax([3,4,12,1,6,7,8,90,9888]));

