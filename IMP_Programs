# 5. Given Number is prime or not.

function isPrime(aa) {
    if (aa<=1) return false
    for ( let i=2;i<=Math.sqrt(aa);i++){
        if (aa%i===0) return false 
    }
    return true   
}
console.log(isPrime(97));
................................................................................................................................. >

# 6. Check if the string is Palindrome
function isPalindrome(str) {
    let res=""
    for ( let i=0;i<=str.length-1;i++) {
        res=str[i]+res;
    }
    if (res===str) {
        return "yes the string is palindrome"
    }
    return "no the string is not palindrome"
}
console.log(isPalindrome("mom"));


const otherString = str.split(”).reverse().join(”); 
return str === otherString;
