# 9. Sorting of 3 Numbers using the nested if 

let a=87
let b=45
let c=77
if ( a>b&& a>c) {
    if (b>c) {
        console.log("First console:",a,b,c)
    }
    else {
        console.log("second console:",a,c,b)
    }
}
else if ( b>c) {
    if (a>c) {
        console.log("third console:",b,a,c)
    }
    else {
        console.log('fourth console:',b,c,a)
    }
}
else {
    if (a>b){
        console.log('fifth console:',c,a,b)
    }
    else {
        console.log('sixth console:',c,b,a)
    }
}

