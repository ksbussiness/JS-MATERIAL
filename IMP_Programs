function nthPrimenumber() {
    let i=2,c=0
    for (;;) {
        let f=true
        for ( let j=2;j<i;j++) {
            if(i%j)==0{
                f=false
                break
            }
        }
        if (f) {
            c=c+1
        }
    if (n==c) {
        console.log('The nth Prime number is :',i);
        break
    }
    
    }
    i++
}

nthPrimenumber(6)






