#.12. finding of the Nth prime number from teh given numbers:

function nthPrimenumber() {
    let i=2,c=0
// infinite loop
    for (;;) {
        let f=true
        for ( let j=2;j<i;j++) {
            if(i%j)==0{
                f=false
                break
            }
        }

        if (f) {
            c=c+1
        }

        if (n==c) {
            console.log('The nth Prime number is :',i);
            break
        }
        i=i+1
    
    }
}
nthPrimenumber(6)

